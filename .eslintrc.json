{
  "extends": ["next/core-web-vitals", "next/typescript", "airbnb"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    "react/require-default-props": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
   ],
   "max-len": ["warn", { "code": 165, "ignoreUrls": true, "ignoreTrailingComments": true }]
  }
}
